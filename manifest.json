{
   "manifest_version": 2,

   "name": "Desk Worker",
   "description": 
      "Quick work out when you are distracted!",
   "version": "1.0",

   "permissions": [
      "notifications", 
      "tabs", 
      "<all_urls>",
      "https://secure.flickr.com/"
   ],
   "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
   }
}