{
  "manifest_version": 2,

  "name": "Desk Trainer",
  "description": "Yay training.",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
    "storage",
    "tabs"
  ]
}
