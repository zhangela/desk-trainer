{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js":["stayfocused.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ]
}
